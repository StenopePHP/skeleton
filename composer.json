{
    "name": "stenope/skeleton",
    "description": "Stenope skeleton for content sites",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11",
        "league/glide-symfony": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "stenope/stenope": "0.x-dev",
        "symfony/asset": "6.4.*@RC",
        "symfony/asset-mapper": "6.4.*@RC",
        "symfony/console": "6.4.*@RC",
        "symfony/dotenv": "6.4.*@RC",
        "symfony/expression-language": "6.4.*@RC",
        "symfony/flex": "^1.17",
        "symfony/framework-bundle": "6.4.*@RC",
        "symfony/intl": "6.4.*@RC",
        "symfony/mime": "6.4.*@RC",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "6.4.*@RC",
        "symfony/property-access": "6.4.*@RC",
        "symfony/property-info": "6.4.*@RC",
        "symfony/runtime": "6.4.*@RC",
        "symfony/serializer": "6.4.*@RC",
        "symfony/stimulus-bundle": "^2.13",
        "symfony/string": "6.4.*@RC",
        "symfony/translation": "6.4.*@RC",
        "symfony/twig-bundle": "6.4.*@RC",
        "symfony/ux-swup": "^2.13",
        "symfony/yaml": "6.4.*@RC",
        "symfonycasts/sass-bundle": "0.2.3",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "ekino/phpstan-banned-code": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpstan/phpstan-symfony": "^1.0",
        "symfony/browser-kit": "6.4.*@RC",
        "symfony/css-selector": "6.4.*@RC",
        "symfony/debug-bundle": "6.4.*@RC",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "6.4.*@RC",
        "symfony/stopwatch": "6.4.*@RC",
        "symfony/var-dumper": "6.4.*@RC",
        "symfony/web-profiler-bundle": "6.4.*@RC"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-create-project-cmd": [
            "npm install --color=always --no-progress --no-audit"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*@RC"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "RC"
}
